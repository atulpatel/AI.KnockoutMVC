<div class="row">
    <div class="span6">
        <div class="well well-large">
            <h4>Non-Observable</h4>
<pre class="alert alert-info">
&lt;div class=&quot;bindable&quot; data-url=&quot;/api/sample&quot; &gt;
...              
&lt;div&gt;
</pre>
            <div class="bindable" data-url="/api/sample">
                <input type="text" data-bind="value:Name.FirstName" />
                <span class="help-inline" data-bind="text:Name.FirstName"></span>
                <br/>
                <input type="text" data-bind="value:Name.LastName" />
                <span class="help-inline" data-bind="text:Name.LastName"></span>
                <br/>
                <input type="text" data-bind="value:Address.Street" />
                <span class="help-inline" data-bind="text:Address.Street"></span>
                <br/>
                <input type="text" data-bind="value:Address.City" />
                <span class="help-inline" data-bind="text:Address.City"></span>
                <br/>
                <input type="text" data-bind="value:Address.State" />
                <span class="help-inline" data-bind="text:Address.State"></span>
                <br/>
                <input type="text" data-bind="value:Phone" />
                <span class="help-inline" data-bind="text:Phone"></span>
            </div>
        </div>
    </div>
<div class="span6">
    <div class="well well-large">
        <h4>Observable</h4>
        <pre class="alert alert-info">
&lt;div class=&quot;bindable observable&quot; data-url=&quot;/api/sample&quot; &gt;
...              
&lt;div&gt;
</pre>
        <div class="bindable observable" data-url="/api/sample">
            <input type="text" data-bind="value:Name().FirstName" />
            <span class="help-inline" data-bind="text:Name().FirstName"></span>
            <br/>
            <input type="text" data-bind="value:Name().LastName" />
            <span class="help-inline" data-bind="text:Name().LastName"></span>
            <br/>
            <input type="text" data-bind="value:Address().Street" />
            <span class="help-inline" data-bind="text:Address().Street"></span>
            <br/>
            <input type="text" data-bind="value:Address().City" />
            <span class="help-inline" data-bind="text:Address().City"></span>
            <br/>
            <input type="text" data-bind="value:Address().State" />
            <span class="help-inline" data-bind="text:Address().State"></span>
            <br/>
            <input type="text" data-bind="value:Phone" />
            <span class="help-inline" data-bind="text:Phone"></span>
        </div>
    </div>
</div>
</div>